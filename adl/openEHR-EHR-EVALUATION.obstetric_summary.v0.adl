definition
	EVALUATION[at0000] matches {	-- Antecedentes obstétricos
		data matches {
			ITEM_TREE[at0001] matches {	-- Arbol
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0014] occurrences matches {0..1} matches {	-- ¿Ha estado embarazada?
						value matches {
							DV_BOOLEAN matches {
								value matches {True}
							}
						}
					}
					ELEMENT[at0002] occurrences matches {0..1} matches {	-- Gravidez
						value matches {
							DV_COUNT matches {
								magnitude matches {|0..<100|}
							}
						}
					}
					ELEMENT[at0003] occurrences matches {0..1} matches {	-- Paridad
						value matches {
							DV_COUNT matches {
								magnitude matches {|>=0|}
							}
						}
					}
					ELEMENT[at0015] occurrences matches {0..1} matches {	-- Nacimientos a término
						value matches {
							DV_COUNT matches {*}
						}
					}
					ELEMENT[at0016] occurrences matches {0..1} matches {	-- Nacimientos pretermino
						value matches {
							DV_COUNT matches {*}
						}
					}
					ELEMENT[at0017] occurrences matches {0..1} matches {	-- Abortos
						value matches {
							DV_COUNT matches {*}
						}
					}
					ELEMENT[at0004] occurrences matches {0..1} matches {	-- Abortos involuntarios
						value matches {
							DV_COUNT matches {
								magnitude matches {|>=0|}
							}
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {	-- Embarazos interrumpidos
						value matches {
							DV_COUNT matches {
								magnitude matches {|>=0|}
							}
						}
					}
					ELEMENT[at0011] occurrences matches {0..1} matches {	-- Embarazos ectópicos
						value matches {
							DV_COUNT matches {*}
						}
					}
					ELEMENT[at0012] occurrences matches {0..1} matches {	-- Nacidos muertos
						value matches {
							DV_COUNT matches {*}
						}
					}
					ELEMENT[at0006] occurrences matches {0..1} matches {	-- Nacidos vivos
						value matches {
							DV_COUNT matches {
								magnitude matches {|>=0|}
							}
						}
					}
					ELEMENT[at0010] occurrences matches {0..1} matches {	-- Cesárea
						value matches {
							DV_COUNT matches {
								magnitude matches {|>=0|}
							}
						}
					}
					ELEMENT[at0018] occurrences matches {0..1} matches {	-- Nacimientos Múltiples (M)
						value matches {
							DV_COUNT matches {*}
						}
					}
					ELEMENT[at0007] occurrences matches {0..1} matches {	-- Nacidos viviendo
						value matches {
							DV_COUNT matches {
								magnitude matches {|>=0|}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0008] matches {	-- Arbol
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0009] occurrences matches {0..1} matches {	-- Fecha de actualización
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0013] occurrences matches {0..1} matches {	-- Definición muerte fetal
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
	}

ontology
	term_definitions = <
		["es"] = <
			items = <
				["at0000"] = <
					text = <"Antecedentes obstétricos">
					description = <"Una visión general de la historia obstétrica de una mujer, incluyendo un resumen de todos los embarazos.">
				>
				["at0001"] = <
					text = <"Arbol">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Gravidez">
					description = <"Número de veces que la mujer ha estado en embarazo, incluida la gestacion actual, si es apropiado">
				>
				["at0003"] = <
					comment = <"las terminaciones tardías del embarazo (después de 20 semanas de gestación) no están incluidos. 

Cuando se registran los Nacimientos a termino (T) y los nacimientos prematuros (P)' , este número debe ser igual a su suma . Nota : La paridad no es considerado como el mismo que el número de niños nacidos - por ejemplo, los gemelos se registraría como un evento de nacimiento.">
					text = <"Paridad">
					description = <"Número de veces que una mujer ha dado a luz después de 20 semanas y 0 días de gestación">
				>
				["at0004"] = <
					comment = <"Incluyendo abortos perdidos">
					text = <"Abortos involuntarios">
					description = <"numero de veces que una mujer ha tenido un aborto involuntario antes de las 20 semanas de gestación">
				>
				["at0005"] = <
					text = <"Embarazos interrumpidos">
					description = <"Número de veces que una mujer ha tenido un embarazo interrumpidos, independientemente de las semanas de gestación">
				>
				["at0006"] = <
					text = <"Nacidos vivos">
					description = <"Número de nacidos Vivos">
				>
				["at0007"] = <
					text = <"Nacidos viviendo">
					description = <"Número de Nacidos que continuan vivos">
				>
				["at0008"] = <
					text = <"Arbol">
					description = <"@ internal @">
				>
				["at0009"] = <
					text = <"Fecha de actualización">
					description = <"Fecha de ultima actualización del resumen">
				>
				["at0010"] = <
					text = <"Cesárea">
					description = <"Número de Cesáreas realizadas">
				>
				["at0011"] = <
					text = <"Embarazos ectópicos">
					description = <"Número de embarazos ectópicos">
				>
				["at0012"] = <
					comment = <"Nota : definición muerte fetal puede estar opcionalmente registrado en el apartado específico en el Protocolo">
					text = <"Nacidos muertos">
					description = <"Numero de Nacidos Muertos">
				>
				["at0013"] = <
					comment = <"útil para comparar datos de diferentes regiones, como las definiciones de muerte fetal pueden variar. Por ejemplo, en Australia un bebé nacido muerto tendrá un embarazo de más de 20 semanas o, si la gestación es desconocida, y pesan más de 400 gramos.">
					text = <"Definición muerte fetal">
					description = <"Definición muerte fetal">
				>
				["at0014"] = <
					comment = <"Registrar como verdadero si nunca ha estado embarazada. Sólo es útil para registrar si no hay otros datos detallados sobre los embarazos anteriores, o donde pueda ser utilizado para apoyar otras actividades basadas en el conocimiento , tales como apoyo a la decisión clínica.">
					text = <"¿Ha estado embarazada?">
					description = <"Embarazos previos">
				>
				["at0015"] = <
					text = <"Nacimientos a término">
					description = <"Numero de recién nacidos de 37 o mas semanas de gestación">
				>
				["at0016"] = <
					text = <"Nacimientos pretermino">
					description = <"Numero de recien nacidos antes de las 37 semanas de gestación">
				>
				["at0017"] = <
					comment = <"Cuando todos los Abortos Involuntarios, Terminaciones y Los embarazos ectópicos son registrados , este número debe ser igual a su suma.">
					text = <"Abortos">
					description = <"Número de embarazos no viables por todas las causas y en cualquier edad gestacional , incluyendo abortos involuntarios , Embarazos interrumpidos y embarazos ectópicos">
				>
				["at0018"] = <
					text = <"Nacimientos Múltiples (M)">
					description = <"Número de nacimientos en los cuales ha nacido mas de un producto vivo">
				>
			>
		>
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Obstetric summary">
					description = <"An overview of the obstetric history of a woman, including a summary of all pregnancies and the associated outcomes or interventions.">
				>
				["at0001"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Gravidity (G)">
					description = <"Number of times a woman has been pregnant, including the current pregnancy, if appropriate.">
				>
				["at0003"] = <
					comment = <"Late terminations of pregnancy (after 20 weeks of gestation) are not included. When both 'Term Births (T)' and 'Preterm Births (P)' are recorded, this number should be equal to their sum. Note: Parity is not regarded as the same as the number of infants born - for example, twins would be recorded as one birth event.">
					text = <"Parity">
					description = <"Number of times a woman has given birth after 20 weeks and 0 days gestation.">
				>
				["at0004"] = <
					comment = <"Including missed abortions.">
					text = <"Miscarriages">
					description = <"Number of times a woman has had a miscarriage at less than 20 weeks of gestation.">
				>
				["at0005"] = <
					text = <"Terminations">
					description = <"Number of times a woman has had a pregnancy terminated, regardless of gestation.">
				>
				["at0006"] = <
					text = <"Live Births">
					description = <"Number of babies born alive.">
				>
				["at0007"] = <
					text = <"Living Children (L)">
					description = <"Number of children who remain alive.">
				>
				["at0008"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0009"] = <
					text = <"Date Updated">
					description = <"The date this summary was last updated.">
				>
				["at0010"] = <
					text = <"Caesarean Sections">
					description = <"Number of Caesarean sections performed.">
				>
				["at0011"] = <
					text = <"Ectopic Pregnancies">
					description = <"Number of ectopic pregnancies.">
				>
				["at0012"] = <
					comment = <"Note: Stillbirth definition may be optionally captured in specific data element in Protocol.">
					text = <"Stillbirths">
					description = <"Number of stillbirths.">
				>
				["at0013"] = <
					comment = <"Maybe useful to record when comparing data from different regions as, as definitions of stillbirth may vary. For example, in Australia a stillborn baby will have a gestation greater than 20 weeks or, if gestation is unknown, weigh more than 400 grams.">
					text = <"Stillbirth Definition">
					description = <"Definition of stillbirth.">
				>
				["at0014"] = <
					comment = <"Record as TRUE if a woman has ever been pregnant. Only useful to record if there is no other detailed data about pregnancies available, or where it may be used to support further knowledge-based activities such as clinical decision support.">
					text = <"Ever Pregnant?">
					description = <"Has the woman ever been pregnant?">
				>
				["at0015"] = <
					text = <"Term Births (T)">
					description = <"Number of infants born on or after 37 weeks of gestation.">
				>
				["at0016"] = <
					text = <"Preterm Births (P)">
					description = <"Number of infants born at less than 37 weeks of gestation.">
				>
				["at0017"] = <
					comment = <"When all of 'Miscarriages', 'Terminations' and 'Ectopic Pregnancies' are recorded, this number should be equal to their sum.">
					text = <"Abortions (A)">
					description = <"Number of non-viable pregnancies from all causes and at any gestation, including miscarriages, induced terminations and ectopic pregnancies.">
				>
				["at0018"] = <
					text = <"Multiple Births (M)">
					description = <"Number of birth events in which more than one fetus has been born.">
				>
			>
		>
		["pt-br"] = <
			items = <
				["at0000"] = <
					text = <"Sumário obstétrico">
					description = <"Uma visão geral da história obstétrica de uma mulher, incluindo um resumo de todas as gestações seus resultados ou as intervenções associadas.

">
				>
				["at0001"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Gestas prévias">
					description = <"Número de vezes que uma mulher esteve grávida, incluindo a gravidez atual, se for o caso.">
				>
				["at0003"] = <
					comment = <"Abortos tardios da gravidez (após 20 semanas de gestação) não estão incluídos. Quando ambos “Nascimento a termo” (T) \"e\" Nascimentos prematuro (P) são gravadas , este número(paridade) deve ser igual a sua soma dos dois. Nota: paridade não é considerado como o mesmo que o número de bebés nascidos - por exemplo , gêmeos seriam registados como um evento nascimento.">
					text = <"Paridade">
					description = <"Número de vezes que uma mulher deu à luz após 20 semanas e 0 dias de gestação.">
				>
				["at0004"] = <
					comment = <"Incluindo abortos retidos.">
					text = <"Aborto espontâneo">
					description = <"Número de vezes que uma mulher teve um aborto espontâneo, com menos de 20 semanas de gestação.">
				>
				["at0005"] = <
					text = <"Aborto induzido">
					description = <"Número de vezes que uma mulher teve uma gravidez terminada, desconsiderando a gestação.">
				>
				["at0006"] = <
					text = <"Nascidos vivos">
					description = <"Número de bebes nascidos vivos.">
				>
				["at0007"] = <
					text = <"Filhos vivos">
					description = <"Número de crianças que permanecem vivas.">
				>
				["at0008"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0009"] = <
					text = <"Data da atualização">
					description = <"Data da última atualização do sumário.">
				>
				["at0010"] = <
					text = <"Cesarianas">
					description = <"Número de cesarianas realizadas.">
				>
				["at0011"] = <
					text = <"Gravidez ectópica">
					description = <"Número de gravidezes ectópicas.">
				>
				["at0012"] = <
					comment = <"Nota: definição de natimorto pode ser opcionalmente capturada no elemento de dados específicos no protocolo.">
					text = <"Natimorto">
					description = <"Número de natimortos">
				>
				["at0013"] = <
					comment = <"Talvez útil para gravar quando se comparam dados de diferentes regiões como, como definições de morte fetal pode variar. Por exemplo , na Austrália um feto morto terá uma gestação superior a 20 semanas ou, no caso de idade gestacional desconhecida, pesar mais de 400 gramas .">
					text = <"Definição de natimorto">
					description = <"Definição de natimorto.">
				>
				["at0014"] = <
					comment = <"Registre como VERDADEIRO se a mulher já está grávida. Só é útil para gravar se não há outros dados detalhados sobre as gravidezes disponíveis, ou onde pode ser utilizado para apoiar atividades baseadas no conhecimento, tais como apoio à decisão clínica.">
					text = <"Já esteve grávida?">
					description = <"A mulher já esteve grávida?">
				>
				["at0015"] = <
					text = <"Nascimentos a termo">
					description = <"Número de bebês nascidos em ou após 37 semanas de gestação.">
				>
				["at0016"] = <
					text = <"Nascimento prematuro">
					description = <"Número de crianças nascidas com menos de 37 semanas de gestação.">
				>
				["at0017"] = <
					comment = <"Quando todos “abortos espontâneos\",\" abortos induzidos\" e \"gravidez ectópicas” são registrados, este número deve ser igual a sua soma.">
					text = <"Abortos">
					description = <"Número de gravidezes não viáveis por todas as causas e em qualquer gestação, incluindo abortos espontâneos, abortos induzidos e gravidezes ectópicas.">
				>
				["at0018"] = <
					text = <"Nascimentos múltiplos">
					description = <"Número de eventos de nascimento (partos) nos quais nasceu mais de um feto.">
				>
			>
		>
	>
