definition
	EVALUATION[at0000.1] matches {	-- Problema/diagnóstico
		data matches {
			ITEM_TREE[at0001] matches {	-- structure
				items cardinality matches {1..*; ordered} matches {
					ELEMENT[at0002.1] matches {	-- Nombre del problema/diagnóstico
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {[ac0.1]}		-- Nombre codificado del diagnostico
							}
						}
					}
					ELEMENT[at0009] occurrences matches {0..1} matches {	-- Descripción clínica
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0012] occurrences matches {0..*} matches {	-- Ubicación anatómica
						value matches {
							DV_TEXT matches {*}
						}
					}
					allow_archetype CLUSTER[at0039] occurrences matches {0..*} matches {	-- Ubicación anatómica estructurada
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.anatomical_location(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.relative_location(-[a-zA-Z0-9_]+)*\.v1/}
					}
					ELEMENT[at0003] occurrences matches {0..1} matches {	-- Momento de reconocimiento del problema
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {	-- Severidad
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0047, 	-- Leve
									at0048, 	-- Moderada
									at0049]	-- Severo
								}
							}
						}
					}
					allow_archetype CLUSTER[at0043] occurrences matches {0..*} matches {	-- Detalles específicos
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0072] occurrences matches {0..1} matches {	-- Progreso
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0030] occurrences matches {0..1} matches {	-- Momento de resolución
						value matches {
							DV_DATE matches {*}
						}
					}
					CLUSTER[at0027] occurrences matches {0..*} matches {	-- Elemento relacionado
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0029] occurrences matches {0..1} matches {	-- Relación
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0040, 	-- Causa
											at0041]	-- Secuencia
										}
									}
								}
							}
							ELEMENT[at0028] occurrences matches {0..1} matches {	-- Elemento
								value matches {
									DV_TEXT matches {*}
									DV_URI matches {*}
								}
							}
							ELEMENT[at0044] occurrences matches {0..1} matches {	-- Descripción
								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
					allow_archetype CLUSTER[at0046] occurrences matches {0..*} matches {	-- Estatus
						include
							archetype_id/value matches {/.*/}
						exclude
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.problem_status(-[a-zA-Z0-9_]+)*\.v1/}
					}
					ELEMENT[at0069] occurrences matches {0..1} matches {	-- Comentario
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0032] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0070] occurrences matches {0..1} matches {	-- Última actualización
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					allow_archetype CLUSTER[at0071] occurrences matches {0..*} matches {	-- Extensión
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

