definition
	ACTION[at0000.1] matches {	-- Procedimiento codificado
		ism_transition matches {
			ISM_TRANSITION[at0034] matches {	-- Procedimiento planificado
				current_state matches {
					DV_CODED_TEXT matches {
						defining_code matches {[openehr::524]}
					}
				}
				careflow_step matches {
					DV_CODED_TEXT matches {
						defining_code matches {[local::at0034]}		-- Procedimiento planificado
					}
				}
			}
			ISM_TRANSITION[at0035] matches {	-- Solicitud de procedimiento enviada
				current_state matches {
					DV_CODED_TEXT matches {
						defining_code matches {[openehr::524]}
					}
				}
				careflow_step matches {
					DV_CODED_TEXT matches {
						defining_code matches {[local::at0035]}		-- Solicitud de procedimiento enviada
					}
				}
			}
			ISM_TRANSITION[at0038] matches {	-- Procedimiento pospuesto
				current_state matches {
					DV_CODED_TEXT matches {
						defining_code matches {[openehr::527]}
					}
				}
				careflow_step matches {
