					}
				}
				careflow_step matches {
					DV_CODED_TEXT matches {
						defining_code matches {[local::at0068]}		-- Procedimiento iniciado
					}
				}
			}
			ISM_TRANSITION[at0047] matches {	-- Procedimiento realizado
